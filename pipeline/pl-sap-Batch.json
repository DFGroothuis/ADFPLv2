{
	"name": "pl-sap-batch",
	"properties": {
		"activities": [
			{
				"name": "ForEachTable",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@variables('table')",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "Execute Full Table Load",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"policy": {
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "plt-sap-FullTableLoad",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"Zone": "Staging",
									"EnvStorage": {
										"value": "@pipeline().globalParameters.gp_adls_environment",
										"type": "Expression"
									},
									"DataSource": "SAP",
									"EnvRegSys": {
										"value": "@pipeline().globalParameters.gp_adls_envregsys",
										"type": "Expression"
									},
									"Domain": "BatchManagement",
									"Object": "Batch",
									"table": {
										"value": "@item()",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"DataSource": {
				"type": "string",
				"defaultValue": "SAP"
			},
			"Domain": {
				"type": "string",
				"defaultValue": "BatchManagement"
			},
			"Object": {
				"type": "string",
				"defaultValue": "Batch"
			}
		},
		"variables": {
			"table": {
				"type": "Array",
				"defaultValue": [
					"MCHA",
					"MCHB",
					"MCH1",
					"MARD"
				]
			}
		},
		"folder": {
			"name": "SAP/sap-BatchManagement"
		},
		"annotations": []
	}
}