									"warehouse": {
										"value": "@pipeline().globalParameters.gp_sf_wh_ingest",
										"type": "Expression"
									},
									"role": {
										"value": "@pipeline().globalParameters.gp_sf_role",
										"type": "Expression"
									}
								}
							},
							"typeProperties": {
								"scripts": [
									{
										"type": "Query",
										"text": {
											"value": "@concat('CALL ', pipeline().globalParameters.gp_sf_database, '.PUBLIC.create_table_sql_from_meta( ''', pipeline().globalParameters.gp_sf_database,''', ''SAP_S4H_LSD200'',''', item(), ''');' )",
											"type": "Expression"
										}
									}
								],
								"scriptBlockExecutionTimeout": "02:00:00"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"table": {
				"type": "array",
				"defaultValue": [
